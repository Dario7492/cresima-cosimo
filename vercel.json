{
  "rewrites": [
    {
      "source": "/style.css",
      "destination": "/style.css"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/apple",
      "destination": "https://apps.apple.com/app/celebrate-condividi-foto/id1231396117",
      "permanent": false
    },
    {
      "source": "/android",
      "destination": "https://play.google.com/store/apps/details?id=app.celebrate.celebrate",
      "permanent": false
    }
  ]
}